        android:theme="@style/AppCompat">

        <activity android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".SceneIntroActivity"
            android:screenOrientation="landscape"/>
        <activity android:name=".Scene1"
            android:screenOrientation="landscape"/>
        <activity android:name=".Scene12"
            android:screenOrientation="landscape"/>
        <activity android:name=".Scene13"

            android:screenOrientation="landscape"/>
        <activity android:name=".Scene14"
            android:screenOrientation="landscape"/>
        <activity android:name=".Scene2"
            android:screenOrientation="landscape"/>
    </application>

</manifest>